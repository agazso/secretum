{
    "compilerOptions": {
        "module": "commonjs",
        "outDir": "build",
        "declaration": false,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "removeComments": true
    }
}